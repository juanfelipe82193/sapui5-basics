<mvc:View height="100%" controllerName="sap.ui.comp.sample.valuehelpdialog.conditionsTabOnly.ValueHelpDialogConditionsTabOnly"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:layout="sap.ui.layout"
	xmlns:form="sap.ui.layout.form"
	xmlns="sap.m">

	<layout:Grid defaultSpan="L12 M12 S12">
		<layout:content>
			<form:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanM="12" labelSpanL="12" labelSpanXL="12" emptySpanL="6" emptySpanM="6">
				<form:content>
					<Label text="Products" labelFor="multiInput"/>
					<MultiInput
						id="multiInput"
						valueHelpOnly="true"
						valueHelpRequest=".onValueHelpRequested"
					>
					</MultiInput>
				</form:content>
			</form:SimpleForm>
		</layout:content>
	</layout:Grid>
</mvc:View>
