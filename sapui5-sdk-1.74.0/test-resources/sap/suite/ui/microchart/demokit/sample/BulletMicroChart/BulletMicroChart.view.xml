			</thresholds>
		</BulletMicroChart>
		<BulletMicroChart scale="M" targetValue="100" forecastValue="110" class="sapUiSmallMargin" press="press" width="16rem">
			<actual>
				<BulletMicroChartData value="120" color="Good"/>
			</actual>
			<thresholds>
				<BulletMicroChartData value="0" color="Error"/>
